{
  "name": "ai-time-machines",
  "version": "2.0.0",
  "description": "Full Stack AI Platform for Time-Series Agent Creation and Management",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js backend/**/*.js",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:python\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "dev:python": "cd python-service && python app.py",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../python-service && pip install -r requirements.txt",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed"
  },
  "keywords": [
    "ai",
    "chatgpt",
    "openai",
    "time-machines",
    "time-series",
    "full-stack",
    "react",
    "express",
    "tensorflow",
    "pytorch",
    "postgresql"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "openai": "^4.20.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
